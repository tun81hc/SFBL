#ifndef SHE_H_
#define SHE_H_

#include "stm32l4xx_hal.h"
#include "aes.h"


void CND_ENC_ECB(struct AES_ctx ctx, uint8_t Key_ID, uint8_t* PlainText, uint8_t* CipherText);
void CND_DEC_ECB(struct AES_ctx ctx, uint8_t* CipherText);
void CND_ENC_CBC(uint8_t Key_ID, uint8_t* PlainText, uint32_t length, uint8_t* HyperText);
void CND_DEC_CBC(uint8_t Key_ID, uint8_t* HyperText, uint32_t length);

void Select_Key(uint8_t Key_ID, unsigned char *Key);


#endif /* SHE_H_ */
