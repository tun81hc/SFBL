#include "SHE.h"
#include "aes.h"
#include "string.h"

#define Nb 4
#define Nk 4        // The number of 32 bit words in a key.
#define Nr 10       // The number of rounds in AES Cipher.

typedef uint8_t state_t[4][4];


void Select_Key(uint8_t Key_ID, unsigned char *Key)
{
 	unsigned char Key1[] = {0x2b,0x7e,0x15,0x16,0x28,0xae,0xd2,0xa6,0xab,0xf7,0x15,0x88,0x09,0xcf,0x4f,0x3c};


 	switch(Key_ID){
 	case 1:
 		memcpy(Key1, Key, 16);
 		break;

 	case 2:
 		break;

 	default:
 		break;

 	}
}

void CND_ENC_ECB(uint8_t Key_ID, uint8_t* PlainText, uint8_t* CipherText)
{
	  unsigned char *Key;
	  struct AES_ctx ctx;

	  Select_Key(Key_ID, Key);
	  AES_init_ctx(&ctx, Key);

	  memcpy((char*)CipherText,(char*)PlainText, 32);
	  Cipher((state_t*)CipherText, ctx->RoundKey);
}


void CND_DEC_ECB(uint8_t Key_ID, uint8_t* CipherText)
{

}




